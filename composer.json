{
  "name": "fedex/crossborder-sdk-php",
  "description": "FedEx Cross Border SDK for PHP",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=5.4",
    "mtdowling/jmespath.php": "~2.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
